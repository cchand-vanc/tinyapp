<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button
      class="navbar-toggler"
      type="button"
      data-toggle="collapse"
      data-target="#navbarNavAltMarkup"
      aria-controls="navbarNavAltMarkup"
      aria-expanded="false"
      aria-label="Toggle navigation"
    >
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
      </div>
    </div>
    <% if(user && user.email){ %>
    <a class="nav-item nav-link">Logged in as: <%= user.email %></a>
    <form method="POST" action="/logout"> 
      <td>
        <button type="submit">Log out</button>
      </td>
    </form>
      <% } else { %>  
      <a class="login" href="/login">
        <button type="submit" style="margin:7px;">Already have an account?</button>
      </a>
      <a class="register" href="/register">
        <button type="submit">Create an account</button>
      </a>
     <% } %>
     </form>
  </nav>
</header>